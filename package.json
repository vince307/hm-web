{
  "name": "hm-web",
  "version": "7.4.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "docker-start": "ng serve  --host 0.0.0.0",
    "build": "ng build",
    "test": "npm run lint && ng test --configuration=test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "build:prod": "ng build --prod --vendor-chunk",
    "clean": "rimraf ./dist/",
    "server": "node ./server.js",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "ci": "npm run clean && npm run format:test && ng lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage && ng e2e && npm run cy:ci && npm run accessibility && npm run build:prod -- --deploy-url /hm-web/ --base-href /hm-web",
    "release": "standard-version && git push --follow-tags origin master",
    "format:write": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate && node .all-contributors-html.js",
    "contributors:check": "all-contributors check",
    "precy:ci": "npm run build:prod -- --deploy-url \"/hm-web/\" --base-href \"/hm-web\"",
    "cy:ci": "npm-run-all --parallel --race server \"cy:run -- --config=baseUrl=http://localhost:4000\"",
    "preaccessibility": "npm run build:prod -- --deploy-url \"/hm-web/\" --base-href \"/hm-web\"",
    "accessibility": "npm-run-all --parallel --race server accessibility:run",
    "accessibility:run": "pa11y-ci --threshold 250",
    "compodoc": "compodoc -p src/tsconfig.app.json",
    "docker:prod": "docker-compose -f Production.docker-compose.yml build",
    "docker:prod-up": "docker-compose -f Production.docker-compose.yml up",
    "docker:prod-down": "docker-compose -f Production.docker-compose.yml down"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,e2e}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.0.3",
    "@angular/http": "^7.0.3",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "@angular/router": "^7.0.3",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@ngrx/effects": "^7.0.0-beta.0",
    "@ngrx/entity": "^7.0.0-beta.0",
    "@ngrx/router-store": "^7.0.0-beta.0",
    "@ngrx/store": "^7.0.0-beta.0",
    "@ngrx/store-devtools": "^7.0.0-beta.0",
    "@ngx-gallery/core": "^4.0.3",
    "@ngx-gallery/lightbox": "^4.0.3",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular-font-awesome": "^3.1.2",
    "angular2_photoswipe": "^7.1.0",
    "angularfire2": "^5.2.1",
    "bootstrap": "^4.3.1",
    "browser-detect": "^0.2.28",
    "core-js": "^2.5.4",
    "filepond": "^4.4.7",
    "filepond-plugin-file-metadata": "^1.0.6",
    "filepond-plugin-file-poster": "^2.0.3",
    "filepond-plugin-file-validate-size": "^2.1.3",
    "filepond-plugin-file-validate-type": "^1.2.4",
    "filepond-plugin-image-preview": "^4.1.1",
    "filepond-plugin-image-validate-size": "^1.2.3",
    "firebase": "^6.2.2",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "material-design-icons": "^3.0.1",
    "ngx-clipboard": "^12.0.0",
    "ngx-cookie-service": "^2.1.0",
    "ngx-filepond": "^5.0.0",
    "ngx-infinite-scroll": "^7.2.0",
    "ngx-masonry": "^1.1.2",
    "ngx-paypal": "^5.0.0",
    "ngx-scroll-event": "^1.0.8",
    "photoswipe": "^4.1.3",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular-extensions/testing-library": "^3.0.3",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.0.3",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.12.2",
    "all-contributors-cli": "^5.4.1",
    "codelyzer": "~4.5.0",
    "express": "^4.17.3",
    "husky": "^1.1.3",
    "jasmine-core": "~3.2.1",
    "jasmine-marbles": "^0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~1.3.1",
    "karma-spec-reporter": "~0.0.32",
    "lint-staged": "^7.3.0",
    "ngrx-store-freeze": "^0.2.4",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^2.1.1",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "rimraf": "^2.6.2",
    "standard-version": "4.2.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "3.1.6",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
